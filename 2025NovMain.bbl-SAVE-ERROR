      \strng{bibnamehash}{f9e1327c815870c55f298e0755f39ab9}
      \strng{authorbibnamehash}{f9e1327c815870c55f298e0755f39ab9}
      \strng{authornamehash}{f9e1327c815870c55f298e0755f39ab9}
      \strng{authorfullhash}{3a22532bbe8ad076bc478d86120d3451}
