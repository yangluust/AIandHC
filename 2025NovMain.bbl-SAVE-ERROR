      \strng{namehash}{843530f70b61e9f3b7f09a5f518760e6}
      \strng{fullhash}{843530f70b61e9f3b7f09a5f518760e6}
      \strng{fullhashraw}{843530f70b61e9f3b7f09a5f518760e6}
      \strng{bibnamehash}{843530f70b61e9f3b7f09a5f518760e6}
      \strng{authorbibnamehash}{843530f70b61e9f3b7f09a5f518760e6}
      \strng{authornamehash}{843530f70b61e9f3b7f09a5f518760e6}
